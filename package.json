{
  "name": "next-stack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:client": "eslint \"*/**/*{tsx,js}\" --ignore-pattern \"pages/api\" --quiet",
    "lint:server": "eslint \"pages/api/**/*{js,ts}\" --config pages/api/.eslintrc  --quiet",
    "lint": "yarn lint:client && yarn lint:server",
    "format": "yarn prettier --write",
    "prettier": "prettier \"*.{js,html,jsx,json,yml,yaml,css,less,scss,ts,tsx,graphql}\"",
    "validate": "yarn lint && yarn --list-different"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn validate"
    }
  },
  "lint-staged": {
    "*.{js,html,jsx,json,yml,yaml,css,less,scss,ts,tsx,graphql}": [
      "npx prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie": "^0.4.1",
    "dotenv": "^8.2.0",
    "faunadb": "^2.14.1",
    "js-cookie": "^2.2.1",
    "next": "9.4.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  }
}
